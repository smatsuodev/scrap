{
  "name": "scrap",
  "type": "module",
  "scripts": {
    "dev": "concurrently -r -k yarn:watch:types vite",
    "build": "$npm_execpath run build:client && $npm_execpath run build:api",
    "build:client": "vite build --mode client",
    "build:api": "vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "$npm_execpath run build && wrangler pages deploy && $npm_execpath run migrate:remote",
    "migrate": "drizzle-kit generate && wrangler d1 migrations apply scrap",
    "migrate:remote": "drizzle-kit generate && wrangler d1 migrations apply scrap --remote ",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "watch:types": "tsc -b -w",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.1",
    "@hookform/resolvers": "^3.9.0",
    "@mantine/core": "^7.13.3",
    "@mantine/form": "^7.13.3",
    "@mantine/hooks": "^7.13.4",
    "@mantine/notifications": "^7.13.3",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tabler/icons-react": "^3.21.0",
    "@tanstack/react-router": "^1.77.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "hono": "^4.6.5",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "shiki": "^1.22.2",
    "ulidx": "^2.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20240529.0",
    "@hono/vite-build": "^1.0.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.15.1",
    "@tanstack/router-devtools": "^1.77.5",
    "@tanstack/router-plugin": "^1.76.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.26.2",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "wrangler": "^3.57.2"
  },
  "packageManager": "yarn@1.22.22"
}
